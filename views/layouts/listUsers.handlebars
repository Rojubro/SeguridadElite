{{> header}}

<div class="container mt-4">
  <div class="row">
    <div class="col-md-12">
      <h1>Lista de Guardas de Seguridad</h1>
      {{#if showGuardasList}}
        <ul>
          {{#each guardas}}
            <li>{{nombre}} {{apellido}} - {{cedula}}</li>
          {{/each}}
        </ul>
      {{/if}}
    </div>
  </div>
</div>

{{> footer}}
